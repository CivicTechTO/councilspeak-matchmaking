<%= form_for :presenter, url: presenters_path do |f| %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :organization %><br />
    <%= f.text_field :organization %>
  </p>

  <p>
    <%= f.label :postal_address %><br />
    <%= f.text_area :postal_address %>
  </p>

  <p>
    <%= f.label :phone %><br />
    <%= f.text_field :phone %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
